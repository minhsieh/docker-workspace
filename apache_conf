        DocumentRoot /var/www/dev/public/
        <Directory /var/www/dev/public/>
                #Options Indexes FollowSymLinks
                AllowOverride all
                Order Allow,deny
                Allow from all
                #Require all granted
        </Directory>
</VirtualHost>

